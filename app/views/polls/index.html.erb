<p id="notice"><%= notice %></p>

<h1>ALL OF THE POLLS</h1>
<h3><%= link_to 'Create New Poll', new_poll_path %></h3>
<table>
  <tr>
    <th>Poll Name</th>
  </tr>

<% @polls.each do |poll| %>
  <tr>
    <td><%= poll.name %></td>
    <td><%= link_to 'Take Poll', poll %></td>
  </tr>
<% end %>
</table>

<br />

