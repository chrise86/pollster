<!DOCTYPE html>
<html>
<head>
  <title>PollsterJy</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container" style= "padding-top:50px">
  <% if flash[:notice] %>
    <div class="alert alert-info">
    <%= flash[:notice] %>
    </div>
  <% end %>

  <% if flash[:error] %>
    <div class="alert alert-error">
    <%= flash[:error] %>
    </div>
  <% end %>


<%= yield %>

</body>
</html>
